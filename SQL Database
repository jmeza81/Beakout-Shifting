create database BreakoutShifting

drop table if exists Interface
drop table if exists Customer
drop table if exists Vehicle 
drop table if exists Dynometer

create table Interface
	(UserID int primary key,
	 VehicleID int,
	 UserFirstName nvarchar(20),
	 UserLastName nvarchar(20),
	 UserPassword nvarchar(20),
	 VehicleMake nvarchar(20),
	 VehicleModel nvarchar(20),
	 VehicleVin varchar (20),
	 Calculations decimal);

create table Customer
	(UserID int primary key,
	 VehicleID int,
	 UserFirstName nvarchar(20),
	 UserLastName nvarchar(20),
	 UserPassword nvarchar(20),
	 VehicleMake nvarchar(20),
	 VehicleModel nvarchar(20),
	 VehicleVin varchar (20),
	 Torque decimal,
	 RPM decimal,
	 SpeedIntervals decimal);

create table Vehicle
	(VehicleID int primary key,
	 VehicleMake nvarchar(20),
	 VehicleModel nvarchar(20),
	 VehicleVin varchar (20))

create table Dynometer
	(UserID int primary key,
	 VehicleID int,
	 Torque decimal,
	 RPM decimal,
	 SpeedIntervals decimal);
